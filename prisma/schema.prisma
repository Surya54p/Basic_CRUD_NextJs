generator client {
  provider = "prisma-client-js"
  // ini kode yang mentaur alamat generate secara custom
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   
  createdAt DateTime @default(now())
}
